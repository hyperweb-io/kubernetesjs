{
  "name": "@interweb/client",
  "version": "0.0.1",
  "description": "Client for interweb",
  "main": "./index.js",
  "types": "./index.d.ts",
  "module": "./esm/index.js",
  "publishConfig": {
    "access": "restricted",
    "directory": "dist"
  },
  "scripts": {
    "copy": "cpy ../../LICENSE README.md package.json dist --flat",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run copy",
    "test": "jest",
    "test:watch": "jest --watch",
    "proxy": "kubectl proxy --port=8001 --accept-hosts='^.*$' --address='0.0.0.0'"
  },
  "dependencies": {
    "@interweb/interwebjs": "workspace:^",
    "@interweb/manifests": "workspace:^",
    "js-yaml": "^4.1.0",
    "kubernetesjs": "^0.7.2",
    "chalk": "^4.1.0",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5"
  }
}
